#!/bin/sh
# PBS -N vector
# PBS -l nodes=1
pssh -h $PBS_NODEFILE rm -rf /home/s2013280/vector 1>&2
pscp -r -h $PBS_NODEFILE /home/s2013280/vector /home/s2013280/ 1>&2
# /home/s2013280/vector/build/bin/ordinary /home/s2013280/vector/conf/in.conf
# /home/s2013280/vector/build/bin/cache /home/s2013280/vector/conf/in.conf
perf record -e L1-dcache-load-misses,L1-dcache-loads,L1-dcache-stores -g -o /home/s2013280/vector/ordinary.perf /home/s2013280/vector/build/bin/ordinary /home/s2013280/vector/conf/in-pbs.conf
perf record -e L1-dcache-load-misses,L1-dcache-loads,L1-dcache-stores -g -o /home/s2013280/vector/misc/cache.perf /home/s2013280/vector/build/bin/cache /home/s2013280/vector/conf/in-pbs.conf
